+#define ADS_PRESSURE_MIN 0
+#define ADS_PRESSURE_MAX 255
+#define ADS_DEBOUNCE_MAX 10
+#define ADS_DEBOUNCE_TOL 3
+#define ADS_DEBOUNCE_REP 1
+#define ADS_KEEP_VREF_ON 1
+#define ADS_SETTLE_DELAY_USECS 150
+#define ADS_WAKEUP false
+#define ADS_VREF_MV 250
+#define ADS_SWAP_XY 0
+
+/*--------------------------------------------------------------------------*/
+#define	MAX_12BIT	((1<<12)-1)
+#define PEN_DOWN_BITMASK 0x1
+
+/* leave ADC powered up (disables penirq) between differential samples */
+#define	READ_Y  0x12
+#define	READ_X  0x11
+#define	READ_Z1 0x13
+#define	READ_Z2 0x14
+#define	READ_PEN_DOWN 0x15
+#define TS_CMD_REG 0x1
+#define TS_FULL_POWER_ON 0xC1
+#define TS_POWER_OFF     0x01
+
+#define TS_POLL_DELAY     1	/* ms delay before the first sample */
+#define TS_POLL_PERIOD  100	/* ms delay between polls */
+#define TS_SAMPLE_PERIOD  5	/* ms delay between samples */
+
+#define TS_X_MIN 0x080
+#define TS_X_MAX 0xf84
+#define TS_Y_MIN 0x089
+#define TS_Y_MAX 0xe90
+
+struct ts_event {
+	/*
+	 * For portability, we can't read 12 bit values using SPI (which
+	 * would make the controller deliver them as native byte order u16
+	 * with msbs zeroed).  Instead, we read them as two 8-bit values,
+	 * *** WHICH NEED BYTESWAPPING *** and range adjustment.
+	 */
+	u16	pos[4];
+
+	bool	ignore;
+	u8	x_buf[3];
+	u8	y_buf[3];
+};
+
+/*
+ * We allocate this separately to avoid cache line sharing issues when
+ * driver is used with DMA-based SPI controllers (like atmel_spi) on
+ * systems where main memory is not DMA-coherent (most non-x86 boards).
+ */
+struct ads7846_packet {
+	u8 			read_cmd[5];
+	
+	u16			dummy;		/* for the pwrdown read */
+	struct ts_event		tc;
+};
+
+struct ads7846 {
+	struct input_dev	*input;
+	char			phys[32];
+	char			name[32];
+
+	struct completion tstask_completion;
+	struct i2c_client	*client;
+	struct regulator	*reg;
+
+	u16			model;
+	u16			vref_mv;
+	u16			vref_delay_usecs;
+	u16			x_plate_ohms;
+	u16			pressure_max;
+
+	bool			swap_xy;
+	bool			use_internal;
+
+	struct ads7846_packet	*packet;
+
+	int			msg_count;
+	wait_queue_head_t	wait;
+
+	bool			pendown;
+
+	int			read_cnt;
+	int			read_rep;
+	int			last_read;
+
+	u16			debounce_max;
+	u16			debounce_tol;
+	u16			debounce_rep;
+
+	u16			penirq_recheck_delay_usecs;
+
+	struct mutex		lock;
+	bool			stopped;	/* P: lock */
+	bool			disabled;	/* P: lock */
+	bool			suspended;	/* P: lock */
+
+	int			(*filter)(void *data, int data_idx, int *val);
+	void			*filter_data;
+	void			(*filter_cleanup)(void *data);
+	int			(*get_pendown_state)(void);
+	int			gpio_pendown;
+
+	void			(*wait_for_sync)(void);
+};
+
+static const struct i2c_device_id tsc_beada_id[] = {
+	{ "tsc-beada", 0 },
+	{ }
+};
+MODULE_DEVICE_TABLE(i2c, tsc_beada_id);
+
+static void tsc_beada_disable(struct ads7846 *ts)
+{
+	mutex_lock(&ts->lock);
+
+	if (!ts->disabled) {
+		ts->disabled = true;
+	}
+
+	mutex_unlock(&ts->lock);
+}
+
+static void tsc_beada_enable(struct ads7846 *ts)
+{
+	mutex_lock(&ts->lock);
+
+	if (ts->disabled) {
+
+		ts->disabled = false;
+	}
+
+	mutex_unlock(&ts->lock);
+}
+
+/* -------------------------------------------------------------------- */
+static void tsc_beada_stop(struct ads7846 *ts)
+{
+	if (!ts->stopped ) {
+
+		/* Signal IRQ thread to stop polling and disable the handler. */
+		tsc_beada_disable(ts);
+		ts->stopped = true;
+		mb();
+		wake_up(&ts->wait);
+	}
+}
+
+static inline int tsc_beada_xfer(struct ads7846 *ts, u8 cmd, u8 data)
+{
+	s32 err;
+
+	err = i2c_smbus_write_byte_data(ts->client, cmd, data);
+	if (err < 0) {
+		dev_err(&ts->client->dev, "i2c io error: %d\n", err);
+	}
+	return err;
+}
+
+static inline int tsc_beada_rcv(struct ads7846 *ts, u8 cmd)
+{
+	s32 data;
+	u16 val;
+
+	data = i2c_smbus_read_word_data(ts->client, cmd);
+	if (data < 0) {
+		dev_err(&ts->client->dev, "i2c io error: %d\n", data);
+		return data;
+	}
+
+	/* The protocol and raw data format from i2c interface:
+	 * S Addr Wr [A] Comm [A] S Addr Rd [A] [DataLow] A [DataHigh] NA P
+	 * Where DataLow has [D11-D4], DataHigh has [D3-D0 << 4 | Dummy 4bit].
+	 */
+	val = swab16(data) >> 4;
+	return val;
+}
+
+static int tsc_beada_read12_ser(struct ads7846 *ts)
+{	
+	int err;
+	
+	err = tsc_beada_xfer(ts, TS_CMD_REG, TS_POWER_OFF);
+	if (err<0)
+		return err;
+		
+	err = tsc_beada_rcv(ts, READ_Y);
+	return (err);
+}
+
+static ssize_t tsc_beada_pen_down_show(struct device *dev,
+				     struct device_attribute *attr, char *buf)
+{
+	struct i2c_client *client = to_i2c_client(dev);
+	struct ads7846 *ts = i2c_get_clientdata(client);
+
+	return sprintf(buf, "%u\n", ts->pendown);
+}
+
+static DEVICE_ATTR(pen_down, S_IRUGO, tsc_beada_pen_down_show, NULL);
+
+static ssize_t tsc_beada_disable_show(struct device *dev,
+				     struct device_attribute *attr, char *buf)
+{
+	struct i2c_client *client = to_i2c_client(dev);
+	struct ads7846 *ts = i2c_get_clientdata(client);
+
+	return sprintf(buf, "%u\n", ts->disabled);
+}
+
+static ssize_t tsc_beada_disable_store(struct device *dev,
+				     struct device_attribute *attr,
+				     const char *buf, size_t count)
+{
+	struct i2c_client *client = to_i2c_client(dev);
+	struct ads7846 *ts = i2c_get_clientdata(client);
+	unsigned long i;
+
+	if (strict_strtoul(buf, 10, &i))
+		return -EINVAL;
+
+	if (i)
+		tsc_beada_disable(ts);
+	else
+		tsc_beada_enable(ts);
+
+	return count;
+}
+
+static DEVICE_ATTR(disable, 0664, tsc_beada_disable_show, tsc_beada_disable_store);
+
+static struct attribute *ads784x_attributes[] = {
+	&dev_attr_pen_down.attr,
+	&dev_attr_disable.attr,
+	NULL,
+};
+
+static struct attribute_group ads784x_attr_group = {
+	.attrs = ads784x_attributes,
+};
+
+/*--------------------------------------------------------------------------*/
+
+static int get_pendown_state(struct ads7846 *ts)
+{
+	s32 data;
+	int val;
+
+	data = i2c_smbus_read_word_data(ts->client, READ_PEN_DOWN);
+	if (data < 0) {
+		dev_err(&ts->client->dev, "i2c io error: %d\n", data);
+	}
+
+	/* The protocol and raw data format from i2c interface:
+	 * S Addr Wr [A] Comm [A] S Addr Rd [A] [DataLow] A [DataHigh] NA P
+	 * Where DataLow has [D11-D4], DataHigh has [D3-D0 << 4 | Dummy 4bit].
+	 */
+	val = swab16(data);
+	return (!(val&PEN_DOWN_BITMASK));
+
+}
+
+static void null_wait_for_sync(void)
+{
+}
+
+static int tsc_beada_debounce_filter(void *ads, int data_idx, int *val)
+{
+	struct ads7846 *ts = ads;
+
+	if (!ts->read_cnt || (abs(ts->last_read - *val) > ts->debounce_tol)) {
+
+		/* Start over collecting consistent readings. */
+		ts->read_rep = 0;
+		/*
+		 * Repeat it, if this was the first read or the read
+		 * wasn't consistent enough.
+		 */
+		if (ts->read_cnt < ts->debounce_max) {
+			ts->last_read = *val;
+			ts->read_cnt++;
+			return ADS7846_FILTER_REPEAT;
+		} else {
+			/*
+			 * Maximum number of debouncing reached and still
+			 * not enough number of consistent readings. Abort
+			 * the whole sample, repeat it in the next sampling
+			 * period.
+			 */
+			dev_dbg(&ts->client->dev, "read_cnt %d last_read %d current_read %d\n",
+			 	ts->read_cnt, ts->last_read, *val);
+			ts->read_cnt = 0;
+			return ADS7846_FILTER_IGNORE;
+		}
+	} else {
+		if (++ts->read_rep > ts->debounce_rep) {
+
+			/*
+			 * Got a good reading for this coordinate,
+			 * go for the next one.
+			 */
+			ts->read_cnt = 0;
+			ts->read_rep = 0;
+			return ADS7846_FILTER_OK;
+		} else {
+
+			/* Read more values that are consistent. */
+			ts->read_cnt++;
+			return ADS7846_FILTER_REPEAT;
+		}
+	}
+}
+
+
+static void tsc_beada_read_state(struct ads7846 *ts)
+{
+	struct ads7846_packet *packet = ts->packet;
+	int msg_idx = 0;
+	int val;
+	int action;
+
+	/* Power on ADS7846 fully during sample */
+	tsc_beada_xfer(ts, TS_CMD_REG, TS_FULL_POWER_ON);
+
+	while (msg_idx < ts->msg_count-1) {
+
+		ts->wait_for_sync();
+
+		/*
+		 * Last message is power down request, no need to convert
+		 * or filter the value.
+		 */
+
+		val = tsc_beada_rcv(ts, packet->read_cmd[msg_idx]);
+
+		action = ts->filter(ts, msg_idx, &val);
+		switch (action) {
+		case ADS7846_FILTER_REPEAT:
+			continue;
+
+		case ADS7846_FILTER_IGNORE:
+			packet->tc.ignore = true;
+			msg_idx = ts->msg_count-1;
+			continue;
+
+		case ADS7846_FILTER_OK:
+			packet->tc.pos[msg_idx] = val;
+			packet->tc.ignore = false;
+			msg_idx++;
+			break;
+
+		default:
+			BUG();
+		} 
+	}
+	
+	/* Leave nPENIRQ active and vREF off, to wait next pen down event */
+	tsc_beada_xfer(ts, TS_CMD_REG, TS_POWER_OFF);
+	val = tsc_beada_rcv(ts, packet->read_cmd[msg_idx]);
+}
+
+static void tsc_beada_report_state(struct ads7846 *ts)
+{
+	struct ads7846_packet *packet = ts->packet;
+	unsigned int Rt;
+	u16 x, y, z1, z2;
+
+	/*
+	 * tsc_beada_get_value() does in-place conversion (including byte swap)
+	 * from on-the-wire format as part of debouncing to get stable
+	 * readings.
+	 */
+	x = packet->tc.pos[1];
+	y = packet->tc.pos[0];
+	z1 = packet->tc.pos[2];
+	z2 = packet->tc.pos[3];
+
+	/* range filtering */
+	if (x == MAX_12BIT)
+		x = 0;
+
+	if (likely(x && z1)) {
+
+		/* compute touch pressure resistance using equation #2 */
+		Rt = z2;
+		Rt -= z1;
+		Rt *= x;
+		Rt *= ts->x_plate_ohms;
+		Rt /= z1;
+		Rt = (Rt + 2047) >> 12;
+	} else {
+		Rt = 0;
+	}
+
+	/*
+	 * Sample found inconsistent by debouncing or pressure is beyond
+	 * the maximum. Don't report it to user space, repeat at least
+	 * once more the measurement
+	 */
+	if (packet->tc.ignore || Rt > ts->pressure_max) {
+		dev_dbg(&ts->client->dev, "ignored %d pressure %d\n",
+			 packet->tc.ignore, Rt);
+		return;
+	}
+
+	/*
+	 * Maybe check the pendown state before reporting. This discards
+	 * false readings when the pen is lifted.
+	 */
+	if (ts->penirq_recheck_delay_usecs) {
+		udelay(ts->penirq_recheck_delay_usecs);
+		if (!get_pendown_state(ts))
+			Rt = 0;
+	}
+
+	/*
+	 * NOTE: We can't rely on the pressure to determine the pen down
+	 * state, even this controller has a pressure sensor. The pressure
+	 * value can fluctuate for quite a while after lifting the pen and
+	 * in some cases may not even settle at the expected value.
+	 *
+	 * The only safe way to check for the pen up condition is in the
+	 * timer by reading the pen signal state (it's a GPIO _and_ IRQ).
+	 */
+	if (Rt) {
+		struct input_dev *input = ts->input;
+
+		if (ts->swap_xy)
+			swap(x, y);
+		
+		x = (ADS_X_MAX*(x-TS_X_MIN))/(TS_X_MAX-TS_X_MIN);
+		y = ADS_Y_MAX-((ADS_Y_MAX*(y-TS_Y_MIN))/(TS_Y_MAX-TS_Y_MIN));
+
+		if (!ts->pendown) {
+			input_report_key(input, BTN_TOUCH, 1);
+			ts->pendown = true;
+			dev_dbg(&ts->client->dev, "DOWN\n");
+		}
+
+		input_report_abs(input, ABS_X, x);
+		input_report_abs(input, ABS_Y, y);
+		input_report_abs(input, ABS_PRESSURE, ts->pressure_max - Rt);
+
+		input_sync(input);
+		dev_dbg(&ts->client->dev, "%4d/%4d/%4d\n", x, y, Rt);
+	}
+}
+
+static irqreturn_t tsc_beada_irq(void *handle)
+{
+	struct ads7846 *ts = handle;
+
+	daemonize(KBUILD_MODNAME);
+	complete(&ts->tstask_completion);
+	
+	while (!ts->stopped) {
+
+		/* Polling period */
+		msleep(TS_POLL_PERIOD);
+					
+		while (!ts->disabled && get_pendown_state(ts)) {
+
+			/* pen is down, continue with the measurement */
+			tsc_beada_read_state(ts);
+
+			if (!ts->disabled)
+				tsc_beada_report_state(ts);
+
+			wait_event_timeout(ts->wait, ts->disabled,
+				   msecs_to_jiffies(TS_SAMPLE_PERIOD));
+
+		}
+
+		if (ts->pendown) {
+			struct input_dev *input = ts->input;
+
+			input_report_key(input, BTN_TOUCH, 0);
+			input_report_abs(input, ABS_PRESSURE, 0);
+			input_sync(input);
+
+			ts->pendown = false;
+			dev_dbg(&ts->client->dev, "UP\n");
+		}
+	}
+	
+	complete_and_exit(&ts->tstask_completion, 0);
+
+	dev_dbg(&ts->client->dev, "thread exit \n");
+	return IRQ_HANDLED;
+}
+
+/*
+ * Set up the transfers to read touchscreen state; this assumes we
+ * use formula #2 for pressure, not #3.
+ */
+static void __devinit tsc_beada_setup_msg(struct ads7846 *ts)
+{
+	struct ads7846_packet *packet = ts->packet;
+
+	ts->msg_count = 5;
+
+	/* y- still on; turn on only y+ (and ADC) */
+	packet->read_cmd[0] = READ_Y;
+	packet->read_cmd[1] = READ_X;
+	packet->read_cmd[2] = READ_Z1;
+	packet->read_cmd[3] = READ_Z2;
+	packet->read_cmd[4] = READ_Y;
+
+}
+
+/*----------------------------------------------------------------------*/
+
+static int __devinit tsc_beada_probe(struct i2c_client *client)
+{
+	struct ads7846 *ts;
+	struct ads7846_packet *packet;
+	struct input_dev *input_dev;
+	struct i2c_adapter *adapter = to_i2c_adapter(client->dev.parent);
+	int err;
+
+	if (!i2c_check_functionality(adapter, I2C_FUNC_SMBUS_BYTE_DATA)
+	    && !i2c_check_functionality(adapter, I2C_FUNC_SMBUS_I2C_BLOCK)) {
+		dev_err(&client->dev, "i2c I/O functions not supported \n");		
+		return -EIO;
+	}
+
+	ts = kzalloc(sizeof(struct ads7846), GFP_KERNEL);
+	packet = kzalloc(sizeof(struct ads7846_packet), GFP_KERNEL);
+	input_dev = input_allocate_device();
+	if (!ts || !packet || !input_dev) {
+		err = -ENOMEM;
+		goto err_free_mem;
+	}
+
+	i2c_set_clientdata(client, ts);
+
+	ts->packet = packet;
+	ts->client = client;
+	ts->input = input_dev;
+	ts->swap_xy = ADS_SWAP_XY;
+	ts->model = ADS_MODEL;
+	ts->vref_delay_usecs = ADS_SETTLE_DELAY_USECS;
+	ts->x_plate_ohms = ADS_X_PLATE_OHMS;
+	ts->pressure_max = ADS_PRESSURE_MAX;
+	ts->debounce_max = ADS_DEBOUNCE_MAX;
+	ts->debounce_tol = ADS_DEBOUNCE_TOL;
+	ts->debounce_rep = ADS_DEBOUNCE_REP;
+	ts->filter = tsc_beada_debounce_filter;
+	ts->disabled = false;
+	ts->stopped = false;
+	ts->pendown = false;
+	ts->wait_for_sync = null_wait_for_sync;
+
+	mutex_init(&ts->lock);
+	init_waitqueue_head(&ts->wait);
+
+	snprintf(ts->phys, sizeof(ts->phys), "%s/input0", dev_name(&client->dev));
+	snprintf(ts->name, sizeof(ts->name), "ADS%d Touchscreen", ts->model);
+
+	input_dev->name = ts->name;
+	input_dev->phys = ts->phys;
+	input_dev->dev.parent = &ts->client->dev;
+
+	input_dev->evbit[0] = BIT_MASK(EV_KEY) | BIT_MASK(EV_ABS);
+	__set_bit(INPUT_PROP_DIRECT, input_dev->propbit);
+	input_dev->keybit[BIT_WORD(BTN_TOUCH)] = BIT_MASK(BTN_TOUCH);
+	input_set_abs_params(input_dev, ABS_X, ADS_X_MIN, ADS_X_MAX, 0, 0);
+	input_set_abs_params(input_dev, ABS_Y, ADS_Y_MIN, ADS_Y_MAX, 0, 0);
+	input_set_abs_params(input_dev, ABS_PRESSURE, ADS_PRESSURE_MIN, ADS_PRESSURE_MAX, 0, 0);
+
+	tsc_beada_setup_msg(ts);
+
+	/*
+	 * Take a first sample, leaving nPENIRQ active and vREF off; avoid
+	 * the touchscreen, in case it's not connected.
+	 */
+	err = tsc_beada_read12_ser(ts);
+	if (err<0) {
+		dev_err(&client->dev, "i2c read error: %d\n", err);		
+		goto err_free_mem;
+	}
+	
+	/*
+	 * Spawn a kernel thread for pen down periodic polling.
+	 */
+	init_completion(&ts->tstask_completion);
+
+	err = kernel_thread((int(*)(void*))tsc_beada_irq, (void*)ts, CLONE_KERNEL);				   
+	wait_for_completion(&ts->tstask_completion);
+
+	dev_info(&client->dev, "thread created: %d\n", err);
+
+	err = sysfs_create_group(&client->dev.kobj, &ads784x_attr_group);
+	if (err)
+		goto err_free_mem;
+
+	err = input_register_device(input_dev);
+	if (err)
+		goto err_remove_attr_group;
+
+	return 0;
+
+ err_remove_attr_group:
+	sysfs_remove_group(&client->dev.kobj, &ads784x_attr_group);
+ err_free_mem:
+	input_free_device(input_dev);
+	kfree(packet);
+	kfree(ts);
+	return err;
+}
+
+static int __devexit tsc_beada_remove(struct i2c_client *client)
+{
+	struct ads7846 *ts = i2c_get_clientdata(client);
+
+	sysfs_remove_group(&client->dev.kobj, &ads784x_attr_group);
+	tsc_beada_stop(ts);
+	input_unregister_device(ts->input);
+	kfree(ts->packet);
+	kfree(ts);
+
+	dev_dbg(&client->dev, "unregistered touchscreen\n");
+
+	return 0;
+}
+
+static struct i2c_driver tsc_beada_driver = {
+	.driver = {
+		.name	= "tsc-beada",
+		.owner	= THIS_MODULE,
+	},
+	.probe		= tsc_beada_probe,
+	.remove		= __devexit_p(tsc_beada_remove),
+	.id_table	= tsc_beada_id,
+};
+
+static int __init tsc_beada_init(void)
+{
+	return i2c_add_driver(&tsc_beada_driver);
+}
+module_init(tsc_beada_init);
+
+static void __exit tsc_beada_exit(void)
+{
+	i2c_del_driver(&tsc_beada_driver);
+}
+module_exit(tsc_beada_exit);
+
+MODULE_DESCRIPTION("Beada Frame touch screen driver");
+MODULE_LICENSE("GPL v2");
+MODULE_ALIAS("platform:beadaframe-tsc");
