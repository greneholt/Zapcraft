--- drivers/video/backlight/pwm-beada.c	1969-12-31 17:00:00.000000000 -0700
+++ drivers/video/backlight/pwm-beada.c	2011-11-06 22:28:48.000000000 -0700
@@ -0,0 +1,314 @@
+/*
+ * Backlight driver for Beada Frame Backlight Devices
+ *
+ * Copyright 2009-2011 www.esky-sh.com.
+ *
+ * Licensed under the GPL-2 or later.
+ */
+ 
+#include <linux/module.h>
+#include <linux/version.h>
+#include <linux/init.h>
+#include <linux/errno.h>
+#include <linux/pm.h>
+#include <linux/platform_device.h>
+#include <linux/i2c.h>
+#include <linux/fb.h>
+#include <linux/backlight.h>
+#include <linux/leds.h>
+#include <linux/workqueue.h>
+#include <linux/slab.h>
+
+#define pwm_beada_EXT_FEATURES
+#define pwm_beada_USE_LEDS
+
+#define PWM_BEADA_MAX_BRIGHTNESS 100
+#define PWM_BEADA_DEVID 0
+#define PWM_BEADA_FREQUENCY_NUM 0x7F
+
+#define PWM_BEADA_CHN_1     0x01
+#define PWM_BEADA_CHN_2     0x02
+#define PWM_BEADA_CHN_3     0x04
+#define PWM_BEADA_CHN_4     0x08
+#define PWM_BEADA_REG_SEL   0x01
+#define PWM_BEADA_REG_1_HI  0x02
+#define PWM_BEADA_REG_1_LOW 0x03
+#define PWM_BEADA_REG_2_HI  0x04
+#define PWM_BEADA_REG_2_LOW 0x05
+#define PWM_BEADA_REG_3_HI  0x06
+#define PWM_BEADA_REG_3_LOW 0x07
+#define PWM_BEADA_REG_4_HI  0x08
+#define PWM_BEADA_REG_4_LOW 0x09
+
+struct beada_bl {
+	struct i2c_client *client;
+	struct backlight_device *bl;
+	struct beada_led *led;
+	struct mutex lock;
+	unsigned long cached_daylight_max;
+	int id;
+	int revid;
+	int current_brightness;
+};
+
+struct beada_led {
+	struct led_classdev	cdev;
+	struct work_struct	work;
+	struct i2c_client	*client;
+	int	new_brightness;
+	int			id;
+	int			flags;
+};
+
+static int pwm_beada_write(struct i2c_client *client, u8 reg, u8 val)
+{
+	int ret = i2c_smbus_write_byte_data(client, reg, val);
+	if (ret)
+		dev_err(&client->dev, "failed to write\n");
+
+	return ret;
+}
+
+/*----------------------------------------------------------------------*/
+
+static int beada_bl_set(struct beada_bl *data, int brightness)
+{
+	int ret = 0;
+	unsigned int temp = brightness;
+
+	if (temp>PWM_BEADA_MAX_BRIGHTNESS)
+		temp=PWM_BEADA_MAX_BRIGHTNESS;
+		
+	temp = temp*PWM_BEADA_FREQUENCY_NUM/PWM_BEADA_MAX_BRIGHTNESS;
+
+	ret = pwm_beada_write(data->client, PWM_BEADA_REG_2_HI,
+				temp);
+	if (ret)
+		return ret;
+
+	ret = pwm_beada_write(data->client, PWM_BEADA_REG_2_LOW,
+				PWM_BEADA_FREQUENCY_NUM-temp);
+	if (!ret)
+		data->current_brightness = brightness;
+
+	return ret;	
+}
+
+static int beada_bl_update_status(struct backlight_device *bl)
+{
+	int brightness;
+	struct beada_bl *data = bl_get_data(bl);
+	
+	brightness = bl->props.brightness;
+	if (bl->props.power != FB_BLANK_UNBLANK)
+		brightness = 0;
+
+	if (bl->props.fb_blank != FB_BLANK_UNBLANK)
+		brightness = 0;
+
+	return beada_bl_set(data, brightness);
+}
+
+static int beada_bl_get_brightness(struct backlight_device *bl)
+{
+	struct beada_bl *data = bl_get_data(bl);
+	return data->current_brightness;
+}
+
+static const struct backlight_ops beada_bl_ops = {
+	.update_status	= beada_bl_update_status,
+	.get_brightness	= beada_bl_get_brightness,
+};
+
+static int beada_bl_init(struct beada_bl *data)
+{
+	int ret = 0;
+
+	ret = pwm_beada_write(data->client, PWM_BEADA_REG_2_LOW, PWM_BEADA_FREQUENCY_NUM);
+	if (ret)
+		return ret;
+
+	ret = pwm_beada_write(data->client, PWM_BEADA_REG_2_HI, 0);
+	if (ret)
+		return ret;
+
+	ret = pwm_beada_write(data->client, PWM_BEADA_REG_SEL, PWM_BEADA_CHN_2);
+	return ret;
+}
+
+static void beada_led_work(struct work_struct *work)
+{
+	struct beada_bl *data; 
+	struct beada_led *led = container_of(work, struct beada_led, work);
+	
+	data = i2c_get_clientdata(led->client);
+	beada_bl_set(data, led->new_brightness);
+}
+
+static void beada_led_set(struct led_classdev *led_cdev,
+			   int led_brightness)
+{
+	struct beada_led *led;
+
+	led = container_of(led_cdev, struct beada_led, cdev);
+	led->new_brightness = led_brightness;
+	schedule_work(&led->work);
+}
+
+static int __devinit beada_led_probe(struct i2c_client *client)
+{
+	struct beada_bl *data = i2c_get_clientdata(client);
+	struct beada_led *led;
+	int ret;
+
+	led = kzalloc(sizeof(*led), GFP_KERNEL);
+	if (led == NULL) {
+		dev_err(&client->dev, "failed to alloc memory\n");
+		return -ENOMEM;
+	}
+
+	led->cdev.name = "lcd-backlight";
+	led->cdev.default_trigger = "none";
+	led->cdev.brightness_set = beada_led_set;
+	led->cdev.brightness = PWM_BEADA_MAX_BRIGHTNESS;
+	led->client = client;
+	led->new_brightness = PWM_BEADA_MAX_BRIGHTNESS;
+	INIT_WORK(&led->work, beada_led_work);
+
+	ret = led_classdev_register(&client->dev, &led->cdev);
+	if (ret) {
+		dev_err(&client->dev, "failed to register LED %d\n",
+			led->id);
+		goto err_free;
+	}
+
+	data->led = led;
+
+	return 0;
+
+ err_free:
+	cancel_work_sync(&led->work);
+	kfree(led);
+
+	return ret;
+}
+
+static int __devexit beada_led_remove(struct i2c_client *client)
+{
+	struct beada_bl *data = i2c_get_clientdata(client);
+
+	led_classdev_unregister(&data->led->cdev);
+	cancel_work_sync(&data->led->work);
+	kfree(data->led);
+	
+	return 0;
+}
+
+/*----------------------------------------------------------------------*/
+
+static int __devinit pwm_beada_probe(struct i2c_client *client,
+					const struct i2c_device_id *id)
+{
+	struct backlight_properties props;
+	struct backlight_device *bl;
+	struct beada_bl *data;
+	int ret;
+
+	if (!i2c_check_functionality(client->adapter,
+					I2C_FUNC_SMBUS_BYTE_DATA)) {
+		dev_err(&client->dev, "SMBUS Byte Data not Supported\n");
+		return -EIO;
+	}
+
+	data = kzalloc(sizeof(*data), GFP_KERNEL);
+	if (data == NULL)
+		return -ENOMEM;
+
+	data->revid = PWM_BEADA_DEVID;
+	data->client = client;
+	data->id = id->driver_data;
+	data->current_brightness = 0;
+	i2c_set_clientdata(client, data);
+
+	mutex_init(&data->lock);
+
+	memset(&props, 0, sizeof(props));
+	props.type = BACKLIGHT_RAW;
+	props.max_brightness = props.brightness = PWM_BEADA_MAX_BRIGHTNESS;
+	bl = backlight_device_register(dev_driver_string(&client->dev),
+			&client->dev, data, &beada_bl_ops, &props);
+	if (IS_ERR(bl)) {
+		dev_err(&client->dev, "failed to register backlight\n");
+		ret = PTR_ERR(bl);
+		goto out1;
+	}
+
+	data->bl = bl;
+
+	ret = beada_bl_init(data);
+	if (ret) {
+		ret = -EIO;
+		goto out;
+	}
+
+	backlight_update_status(bl);
+	dev_info(&client->dev, "Rev.%d Backlight\n", data->revid);
+
+	beada_led_probe(client);
+	return 0;
+
+out:
+	backlight_device_unregister(bl);
+out1:
+	i2c_set_clientdata(client, NULL);
+	kfree(data);
+
+	return ret;
+}
+
+/*----------------------------------------------------------------------*/
+
+static int __devexit pwm_beada_remove(struct i2c_client *client)
+{
+	struct beada_bl *data = i2c_get_clientdata(client);
+
+	beada_led_remove(client);
+	backlight_device_unregister(data->bl);
+	i2c_set_clientdata(client, NULL);
+	kfree(data);
+
+	return 0;
+}
+
+static const struct i2c_device_id pwm_beada_id[] = {
+	{ "pwm-beada", 0 },
+	{ }
+};
+MODULE_DEVICE_TABLE(i2c, pwm_beada_id);
+
+static struct i2c_driver pwm_beada_driver = {
+	.driver = {
+		.name = KBUILD_MODNAME,
+	},
+	.probe    = pwm_beada_probe,
+	.remove   = __devexit_p(pwm_beada_remove),
+	.suspend = NULL,
+	.resume  = NULL,
+	.id_table = pwm_beada_id,
+};
+
+static int __init pwm_beada_init(void)
+{
+	return i2c_add_driver(&pwm_beada_driver);
+}
